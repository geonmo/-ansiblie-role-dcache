dcache.enable.space-reservation = false
dcache.systemd.strict=false
dcache.zookeeper.connection = belle-se2-head.sdfarm.kr:2181
dcache.java.memory.heap=4096m
[zkDomain]
[zkDomain/zookeeper]

[dCacheDomain]
dcache.broker.scheme = core
[dCacheDomain/admin]
[dCacheDomain/poolmanager]
[dCacheDomain/topo]
[dCacheDomain/pnfsmanager]
pnfsmanager.default-retention-policy = REPLICA
pnfsmanager.default-access-latency = ONLINE
[dCacheDomain/nfs]
nfs.version = 4.1
nfs.loginbroker.tags = dcache-nfs

[dCacheDomain/gplazma]
[dCacheDomain/pinmanager]
[dCacheDomain/billing]
[dCacheDomain/httpd]
[dCacheDomain/cleaner]
[dCacheDomain/xrootd]
xrootd.net.port=1094
xrootd.authz.read-paths = /
xrootd.authz.write-paths = /
